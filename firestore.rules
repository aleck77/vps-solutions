
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: This rule is insecure for production.
    // It allows anyone to read and write to your database.
    // TODO: Secure your rules before deploying to production.
    // For development and seeding, we allow all reads and writes.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
